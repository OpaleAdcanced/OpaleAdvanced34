<?xml version="1.0" encoding="UTF-8"?>
<sm:binaryOdTransf xmlns:sc="http://www.utc.fr/ics/scenari/v3/core" xmlns:sm="http://www.utc.fr/ics/scenari/v3/modeling">
	<sm:model sc:refUri="/binaries/jpg/jpeg.model"/>
	<sm:axis code="inline"/>
	<sm:content>
		<sm:WImage globalStyle="viewPictureInline">
			<sm:binaryUrl>
				<sm:imageTransform maxTargetDpi="300">
					<sm:scaleFromStandardCharSize targetFontSizeInPt="10">
						<sm:ifNoStandardCharSize>
							<sm:scale ratio="1" sourceDpiIfNone="300"/>
						</sm:ifNoStandardCharSize>
					</sm:scaleFromStandardCharSize>
					<sm:dimBounds unit="cm" maxWidth="10" maxHeight="2"/>
					<sm:outputJPEG/>
				</sm:imageTransform>
			</sm:binaryUrl>
		</sm:WImage>
	</sm:content>
</sm:binaryOdTransf>