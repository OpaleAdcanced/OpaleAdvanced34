	<sm:targetModel>
		<sm:current/>
		<sm:ifNone>
			<sm:fixedResult return="false"/>
		</sm:ifNone>
	</sm:targetModel>
	<sm:expression>
		<sm:freeXpath xpath=" typeAgent(concat('@',rootModel())) = 'op_expUc'"/>
	</sm:expression>
</sm:genericCond>