<?xml version="1.0" encoding="UTF-8"?>
<sm:textPrim xmlns:sm="http://www.utc.fr/ics/scenari/v3/modeling" xmlns:sc="http://www.utc.fr/ics/scenari/v3/core" xmlns:sext="http://www.utc.fr/ics/scenari/v3/modeling/extension" name="￼;Texte￼">
	<sm:identification targetNamespace="utc.fr:ics/opale3" targetPrefix="op" code="bibTxt"/>
	<sm:structure>
		<sm:inlineTags>
			<sm:uLinkTag role="url" name="￼;Lien vers la source web￼">
				<sm:externalUrl/>
			</sm:uLinkTag>
			<sm:textLeafTag role="title" name="￼;Titre principal, titre de la publication￼"/>
