<?xml version="1.0" encoding="UTF-8"?>
<sm:dataFormPrim xmlns:sc="http://www.utc.fr/ics/scenari/v3/core" xmlns:sm="http://www.utc.fr/ics/scenari/v3/modeling" name="￼;Paramétrage des synonymes &amp; options￼">
	<sm:identification targetNamespace="utc.fr:ics/opale3" targetPrefix="op" code="gapM" itemExtension="xml"/>
	<sm:structure>
		<sm:group code="synonyms" name="￼;Synonymes￼" occurence="zeroOrOne">
			<sm:help quickHelp="￼;Ajoutez ici une liste de synonymes du mot présent dans le trou. Chaque synonyme est considéré comme une réponse juste.￼"/>
			<sm:field code="synonym" name="￼;Synonyme￼" occurence="zeroOrMore">
				<sm:string/>
			</sm:field>
		</sm:group>
		<sm:group code="options" name="￼;Options￼" occurence="zeroOrOne">
			<sm:help quickHelp="￼;Liste des options proposée telle qu'elle à l'apprenant. ATTENTION : le mot présent dans le trou ainsi que ses synonymes doivent être reportés dans les options.￼"/>
